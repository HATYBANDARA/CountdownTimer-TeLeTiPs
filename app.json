{
    "name": "TimeCalculate-Edu_bot",
    "description": "Powerful Telegram bot to Calculate to your important events & Times in any chat.",
    "repository": "https://github.com/HATYBANDARA/TimeCalculate-Edu_bot",
    "logo": "https://t.me/technology_ALL_Subject_Quiz_Gr/232148",
    "keywords": [
        "countdown",
        "timer",
        "important events",
        "countdown timer",
        "Telegram bot",
        "Pyrogram bot"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Get from Telegram's @BotFather",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
